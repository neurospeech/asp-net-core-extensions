<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <ApplicationIcon />
    <OutputType>Library</OutputType>
    <PackageId>NeuroSpeech.NodePackageService</PackageId>
    <Version>1.0.1</Version>
    <StartupObject />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Akash Kava</Authors>
    <Company>NeuroSpeech Technologies Pvt Ltd</Company>
    <Product>NeuroSpeech NodePackageService</Product>
    <DebugSymbols>true</DebugSymbols>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Jering.Javascript.NodeJS" Version="5.3.0"/>
    <PackageReference Include="AngleSharp" Version="0.9.11" />
    <PackageReference Include="MimeKit" Version="2.4.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="SharpZipLib" Version="1.2.0" />
    <PackageReference Include="System.Diagnostics.Process" Version="4.3.0" />
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="3.0.0" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\..\StringExtensions\StringExtensions.csproj" />
    <ProjectReference Include="..\NeuroSpeech.NodePackageInstaller\NeuroSpeech.NodePackageInstaller.csproj" />
  </ItemGroup>
  

</Project>
